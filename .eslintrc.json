{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "yoda": 0,
    "semi": [2, "always"],
    "no-extra-semi": 2,
    "semi-spacing": [2, { "before": false, "after": true }],
    "no-shadow": [2, {"builtinGlobals": true, "hoist": "functions"}]
  }
}
